@model IEnumerable<SystemRezerwacji.Models.Relacja>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stacja_poczatkowa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stacja_koncowa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Stacja_posrednia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Odleglosc)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stacja_poczatkowa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stacja_koncowa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Stacja_posrednia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Odleglosc)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
