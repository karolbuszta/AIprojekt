@model IEnumerable<SystemRezerwacji.Models.Polaczenie>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Relacje.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Czas_odjazdu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Czas_przyjazdu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Kategoria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cena)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Relacje.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Czas_odjazdu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Czas_przyjazdu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Kategoria)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cena)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
